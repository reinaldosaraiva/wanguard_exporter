# WANGuard API Configuration
# Copy this file to .env and fill in your values

# WANGuard API endpoint
WANGUARD_ADDRESS=http://your-wanguard-server:81

# WANGuard API credentials
WANGUARD_USERNAME=admin
WANGUARD_PASSWORD=your-secure-password

# Exporter configuration (optional)
WANGUARD_METRICS_PATH=/metrics
WANGUARD_LISTEN_ADDRESS=:9868

# Grafana credentials (optional, for grafana profile)
GRAFANA_USER=admin
GRAFANA_PASSWORD=admin
